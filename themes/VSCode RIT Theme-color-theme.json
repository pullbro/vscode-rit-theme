{
	"name": "RIT Theme for VS Code",
	"colors": {
		"editor.background": "#F76902",
		"editor.foreground": "#333333",
		"list.activeSelectionIconForeground": "#F76902"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#000000"
			}
		},
		{
			"name": "Preprocessor Comments",
			"scope": "comment.block.preprocessor",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Documentation Comments",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "HTML Tags",
			"scope": [
				"meta.tag",
				"punctuation.definition.tag.html",
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#000000"
			}
		},
		{
			"name": "Bold Text",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Italic Text",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline",
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Headings",
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Links",
			"scope": "meta.link",
			"settings": {
				"foreground": "#F76902"
			}
		}
	]
}